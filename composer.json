{
    "name": "robert/poz",
    "description": "Poz package for Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Robert\\Poz\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robert\\Poz\\PozServiceProvider",
                "Robert\\Poz\\PozServiceLoader"
            ],
            "aliases": {
                "Poz": "Robert\\Poz\\Facades\\Poz"
            }
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0"
    }
}
